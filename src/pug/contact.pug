- var PageTitle = 'Contac Us - ICONS Agency';
doctype
html
    head
        include partials/head
        include partials/style
    body

        //- Create Preload Page

        #preloader
            .loading LOADING ...

        include partials/btn-top

        .fixed-breadcrumb
            strong &larr; &nbsp; Contact Us 
            | â€” Overview

        include partials/header

        section.contact-banner.showcase
            .container
                .inner-banner
                    h1.display-4 Contact Us
                    p Contact us to get the best offers

        section.contact-section
            .container
                h2.text-center Send Us a Message
                h4.text-center.lead We Look Forward To You
                hr
                .row.m-0.py-5
                    .col-xl-5.p-0
                        .contact-image(data-aos='fade-right')
                            img(src='img/contact/contact-img.jpg' alt='contact image' draggable='false')
                    .col-xl-7.p-0
                        .row.m-0.form-contact(data-aos='fade-left')
                            .col-sm-6
                                .form-group
                                    input.form-control(type='text', name='name')
                                    label Name
                                    span.help-block This Field Is Required
                            .col-sm-6
                                .form-group
                                    input.form-control(type='email', name='email')
                                    label Email
                                    span.help-block This Field Is Required
                            .col-sm-6
                                .form-group
                                    input.form-control(type='text', name='phone')
                                    label Phone
                                    span.help-block This Field Is Required
                            .col-sm-6
                                .form-group
                                    input.form-control(type='text', name='website')
                                    label Website
                                    span.help-block This Field Is Required
                            .col-sm-12
                                .form-group
                                    textarea.form-control(name='message', rows='5')
                                    label Message
                                    span.help-block This Field Is Required
                            .col-sm-6
                                .btn-wrapper
                                    button.submit-form(type='submit') Contact us now !
                                            .contact-info-box.text-center
                            ul.col-sm-6.social-links.d-flex.justify-content-end
                                li
                                    a.facebook(title='ICONS on Facebook', href='https://www.facebook.com/egicons', target='_blank')
                                        span.fab.fa-facebook-f
                                li
                                    a.twitter(title='ICONS on Twitter', href='https://twitter.com/egicons', target='_blank')
                                        span.fab.fa-twitter
                                li
                                    a.instagram(title='ICONS on Instagram', href='https://www.instagram.com/iconsegypt/', target='_blank')
                                        span.fab.fa-instagram
                                li
                                    a.youtube(title='ICONS on Youtube', href='https://www.youtube.com/channel/UC2mKNKvudOHYOWi2Xif2lZQ?view_as=subscriber', target='_blank')
                                        span.fab.fa-youtube
                                li
                                    a.linkedin(title='ICONS on Linkedin', href='https://www.instagram.com/iconsegypt/', target='_blank')
                                        span.fab.fa-linkedin

        section.map-location.text-center
            h2(data-aos='zoom-in') Visit Our Work Places
            hr
            .row(data-aos='zoom-in')
                .col-sm-12.my-3
                    .container
                        ul.contact-us-tab#myTab.nav.nav-tabs(role='tablist')
                            li.nav-item.col-md-3.col-6
                                a.nav-link(href='#GB', data-id='#GB', aria-controls='GB', role='tab', data-toggle='tab', aria-selected='true')
                                    .country
                                        img(src='https://png.icons8.com/africa-filled/ios7/45/000000' alt='icon' draggable='false')
                                    p London
                            li.nav-item.col-md-3.col-6
                                a.nav-link.active(href='#EG', data-id='#EG', aria-controls='EG', role='tab', data-toggle='tab', aria-selected='true')
                                    .country
                                        img(src='https://png.icons8.com/africa-filled/ios7/45/000000' alt='icon' draggable='false')
                                    p Egypt Office
                            li.nav-item.col-md-3.col-6
                                a.nav-link(href='#KW', data-id='#KW', aria-controls='KW', role='tab', data-toggle='tab', aria-selected='true')
                                    .country
                                        img(src='https://png.icons8.com/africa-filled/ios7/45/000000' alt='icon' draggable='false')
                                    p Kuwait Office
                            li.nav-item.col-md-3.col-6
                                a.nav-link(href='#SA', data-id='#SA', aria-controls='SA', role='tab', data-toggle='tab', aria-selected='true')
                                    .country
                                        img(src='https://png.icons8.com/africa-filled/ios7/45/000000' alt='icon' draggable='false')
                                    p Saudi Arabia Office
                #myTabContent.tab-content.col-sm-12
                    #GB.tab-pane(role='tabpanel')
                        p Cairo, Maadi, Street 23, Building No. 10, 6th Floor.
                        iframe(src='https://www.google.com/maps/embed/v1/place?q=24.7136,46.6753&key=AIzaSyDsshcWfauVkoW5bbTJHpMQG9aiOUnuygA', width='100%', height='300', frameborder='0', style='border:0;', allowfullscreen='')
                    #EG.tab-pane.active(role='tabpanel')
                        p Cairo, Maadi, Street 23, Building No. 10, 6th Floor.
                        iframe(src='https://www.google.com/maps/embed/v1/place?q=30.01345141,31.21033911&key=AIzaSyDsshcWfauVkoW5bbTJHpMQG9aiOUnuygA', width='100%', height='300', frameborder='0', style='border:0;', allowfullscreen='')
                    #KW.tab-pane(role='tabpanel')
                        p Cairo, Maadi, Street 23, Building No. 10, 6th Floor.
                        iframe(src='https://www.google.com/maps/embed/v1/place?q=24.7136,46.6753&key=AIzaSyDsshcWfauVkoW5bbTJHpMQG9aiOUnuygA', width='100%', height='300', frameborder='0', style='border:0;', allowfullscreen='')
                    #SA.tab-pane(role='tabpanel')
                        p Cairo, Maadi, Street 23, Building No. 10, 6th Floor.
                        iframe(src='https://www.google.com/maps/embed/v1/place?q=24.7136,46.6753&key=AIzaSyDsshcWfauVkoW5bbTJHpMQG9aiOUnuygA', width='100%', height='300', frameborder='0', style='border:0;', allowfullscreen='')





        include partials/footer
        include partials/footerScripts
